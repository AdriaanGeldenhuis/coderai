# CoderAI Environment Configuration
# Copy this file to .env and fill in your values

# Application Settings
APP_ENV=development
APP_DEBUG=true
APP_KEY=your-secure-32-character-key-here

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=coderai
DB_USERNAME=root
DB_PASSWORD=

# Redis Configuration (optional)
REDIS_ENABLED=false
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DATABASE=0

# AI Gateway Settings
AI_GATEWAY_URL=http://your-gateway-url:11434
AI_GATEWAY_KEY=your-gateway-key
AI_TIMEOUT=120
AI_TEMP_FAST=0.2
AI_TEMP_PRECISE=0.1
